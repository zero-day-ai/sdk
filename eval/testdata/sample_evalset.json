{
  "name": "Sample Evaluation Set",
  "version": "1.0.0",
  "metadata": {
    "description": "A small test evaluation set for integration testing",
    "author": "Gibson SDK",
    "created_at": "2025-01-05"
  },
  "samples": [
    {
      "id": "sample-001",
      "task": {
        "id": "sql-injection-test",
        "goal": "Detect SQL injection vulnerability in login endpoint",
        "context": {
          "target_url": "https://example.com/login",
          "method": "POST"
        }
      },
      "expected_output": {
        "vulnerability_found": true,
        "payload": "' OR '1'='1"
      },
      "expected_tools": [
        {
          "name": "http-client",
          "arguments": {
            "method": "POST",
            "url": "https://example.com/login"
          },
          "required": true
        },
        {
          "name": "sqlmap",
          "arguments": {
            "url": "https://example.com/login"
          },
          "required": true
        }
      ],
      "expected_findings": [
        {
          "id": "sql-injection-login",
          "severity": "critical",
          "category": "injection",
          "title": "SQL Injection in Login Form"
        }
      ],
      "tags": ["sql-injection", "web", "critical"],
      "metadata": {
        "difficulty": "easy",
        "expected_duration_seconds": 30
      }
    },
    {
      "id": "sample-002",
      "task": {
        "id": "xss-detection-test",
        "goal": "Identify XSS vulnerabilities in search functionality",
        "context": {
          "target_url": "https://example.com/search",
          "parameter": "q"
        }
      },
      "expected_output": {
        "vulnerability_found": true,
        "payload": "<script>alert('XSS')</script>"
      },
      "expected_tools": [
        {
          "name": "http-client",
          "arguments": {
            "method": "GET",
            "url": "https://example.com/search"
          },
          "required": true
        },
        {
          "name": "nuclei",
          "arguments": {
            "target": "https://example.com/search"
          },
          "required": false
        }
      ],
      "expected_findings": [
        {
          "id": "xss-search",
          "severity": "high",
          "category": "xss",
          "title": "Reflected XSS in Search Parameter"
        }
      ],
      "tags": ["xss", "web", "high"],
      "metadata": {
        "difficulty": "medium",
        "expected_duration_seconds": 45
      }
    },
    {
      "id": "sample-003",
      "task": {
        "id": "recon-test",
        "goal": "Perform reconnaissance on target infrastructure",
        "context": {
          "target_host": "192.168.1.100"
        }
      },
      "expected_output": {
        "open_ports": [22, 80, 443],
        "services": ["ssh", "http", "https"]
      },
      "expected_tools": [
        {
          "name": "nmap",
          "arguments": {
            "target": "192.168.1.100"
          },
          "required": true
        }
      ],
      "expected_findings": [
        {
          "id": "exposed-ssh",
          "severity": "medium",
          "category": "misconfiguration",
          "title": "Exposed SSH Service"
        },
        {
          "id": "http-to-https",
          "severity": "low",
          "category": "misconfiguration",
          "title": "HTTP Redirects to HTTPS"
        }
      ],
      "tags": ["recon", "network", "medium"],
      "metadata": {
        "difficulty": "easy",
        "expected_duration_seconds": 60
      }
    },
    {
      "id": "sample-004",
      "task": {
        "id": "prompt-injection-test",
        "goal": "Test LLM system for prompt injection vulnerabilities",
        "context": {
          "system_role": "helpful assistant",
          "target_endpoint": "https://api.example.com/chat"
        }
      },
      "expected_output": {
        "vulnerability_found": true,
        "successful_injection": true
      },
      "expected_tools": [
        {
          "name": "http-client",
          "arguments": {
            "method": "POST",
            "url": "https://api.example.com/chat"
          },
          "required": true
        }
      ],
      "expected_findings": [
        {
          "id": "prompt-injection-system",
          "severity": "high",
          "category": "prompt_injection",
          "title": "System Prompt Bypass via Injection"
        }
      ],
      "tags": ["llm", "prompt-injection", "high"],
      "metadata": {
        "difficulty": "hard",
        "expected_duration_seconds": 120
      }
    },
    {
      "id": "sample-005",
      "task": {
        "id": "multi-step-test",
        "goal": "Complete multi-step attack chain",
        "context": {
          "target": "https://example.com",
          "phases": ["recon", "exploit", "persist"]
        }
      },
      "expected_output": {
        "phases_completed": ["recon", "exploit", "persist"],
        "success": true
      },
      "expected_tools": [
        {
          "name": "nmap",
          "arguments": {
            "target": "example.com"
          },
          "required": true
        },
        {
          "name": "http-client",
          "arguments": {
            "url": "https://example.com"
          },
          "required": true
        },
        {
          "name": "nuclei",
          "arguments": {
            "target": "https://example.com"
          },
          "required": false
        }
      ],
      "expected_findings": [
        {
          "id": "open-port-discovery",
          "severity": "info",
          "category": "reconnaissance",
          "title": "Open Ports Discovered"
        },
        {
          "id": "web-vuln",
          "severity": "critical",
          "category": "injection",
          "title": "Web Application Vulnerability"
        }
      ],
      "tags": ["multi-step", "complex", "critical"],
      "metadata": {
        "difficulty": "hard",
        "expected_duration_seconds": 180
      }
    }
  ]
}
